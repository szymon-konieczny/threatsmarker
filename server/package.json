  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-pino-logger": "^4.0.0",
    "pg": "^7.14.0",
    "pino": "^5.14.0",
    "pino-pretty": "^3.5.0",
